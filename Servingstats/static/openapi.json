{
  "openapi": "3.0.0",
  "info": {
    "version": "1.4.0",
    "title": "spc-spec",
    "contact": {
      "name": "DOTZERO",
      "email": "info@dotzero.tech"
    },
    "description": "An API to spc"
  },
  "servers": [
    {
      "url": "http://localhost:5000"
    }
  ],
  "paths": {
    "/v1/nelson": {
      "get": {
        "tags": [
          "statistics"
        ],
        "summary": "取得統計結果",
        "operationId": "getSpec",
        "parameters": [
          {
            "name": "startTime",
            "in": "query",
            "description": "工序報工歷史資料(workOrderHistory) 的開工時間 2020-05-20T16:26:46Z\n",
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "example": "2020-05-20T16:26:46Z"
          },
          {
            "name": "endTime",
            "in": "query",
            "description": "工序報工歷史資料(workOrderHistory) 的完工時間 2020-05-20T16:26:46Z\n",
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "example": "2021-01-15T10:47:32Z"
          },
          {
            "name": "spcMeasurePointConfigUUID",
            "in": "query",
            "description": "量測點參數的UUID\n",
            "schema": {
              "type": "string",
              "format": "uuid",
              "required": [
                "uuid"
              ]
            },
            "example": "7202a1e7-346d-11eb-8118-3ee8fbec59ed"
          },
          {
            "name": "workOrderOpHistoryUUID",
            "in": "query",
            "description": "工序報工歷史資料的UUID\n",
            "schema": {
              "type": "string",
              "format": "uuid"
            },
            "example": "4134d124-ddb0-4ef4-91ba-922710ec7d65"
          }
        ],
        "responses": {
          "200": {
            "description": "統計結果",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "default": {
            "description": "unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/v1/capability": {
      "get": {
        "tags": [
          "statistics"
        ],
        "summary": "取得統計結果",
        "operationId": "getCapability",
        "parameters": [
          {
            "name": "startTime",
            "in": "query",
            "description": "工序報工歷史資料(workOrderHistory) 的開工時間 2020-05-20T16:26:46Z\n",
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "example": "2020-05-20T16:26:46Z"
          },
          {
            "name": "endTime",
            "in": "query",
            "description": "工序報工歷史資料(workOrderHistory) 的完工時間 2020-05-20T16:26:46Z\n",
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "example": "2021-01-15T10:47:32Z"
          },
          {
            "name": "spcMeasurePointConfigUUID",
            "in": "query",
            "description": "量測點參數的UUID\n",
            "schema": {
              "type": "string",
              "format": "uuid",
              "required": [
                "uuid"
              ]
            },
            "example": "7202a1e7-346d-11eb-8118-3ee8fbec59ed"
          },
          {
            "name": "workOrderOpHistoryUUID",
            "in": "query",
            "description": "工序報工歷史資料的UUID\n",
            "schema": {
              "type": "string",
              "format": "uuid"
            },
            "example": "4134d124-ddb0-4ef4-91ba-922710ec7d65"
          }
        ],
        "responses": {
          "200": {
            "description": "統計結果",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          },
          "default": {
            "description": "unexpected error",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "statistics",
      "description": "檢查nelson規則 && 量測點統計製程指標"
    }
  ]
}